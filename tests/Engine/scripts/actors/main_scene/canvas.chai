var canvas := stage.create_actor()

var window_size := core.video.window.get_size()

var panel_origin := vec3(0.0f, 0.0f, 0.0f)
var panel_size := vec3(window_size.x / 3.0f * 2.0f, window_size.y / 3.0f * 2.0f, 0.0f)


panel_origin.x += panel_size.x / 4.0f
panel_origin.y += panel_size.y / 4.0f
//panel_origin.x = preview_panel_x + 250.0f
//panel_origin.x += x_offset


//panel_origin.x += x_offset
//panel_origin.y -= y_offset

canvas.add_model("glass_window",
    panel_origin,
    panel_size,
    "cristal_panel.json")

canvas.set_origin(panel_origin)
canvas.set_size(panel_size)

return canvas