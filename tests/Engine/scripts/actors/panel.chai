class panel{
    attr widget

    def panel(origin, size){
        this.widget := stage.create_actor()
        this.widget.set_origin(origin)
        this.widget.set_size(util.size_from_normal(size)))

        this.widget.add_model("panel", origin, util.size_from_normal(size)), "panel.json")
    }

    def get(){
        return this.widget
    }

}

return fun(origin, size){
    return panel(origin, size)
}